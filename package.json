{
  "name": "@cherrol/qrcode",
  "version": "0.1.0",
  "description": "QRCode.js is javascript library for making QRCode. QRCode.js supports Cross-browser with HTML5 Canvas and table tag in DOM. QRCode.js has no dependencies.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cherrol/qrcodejs.git"
  },
  "bugs": {
    "url": "https://github.com/cherrol/qrcodejs/issues"
  },
  "homepage": "https://github.com/cherrol/qrcodejs#readme",
  "main": "src/index.js",
  "author": "cherrol",
  "license": "MIT",
  "keywords": [
    "share",
    "javascript"
  ],
  "scripts": {
    "watch": "rimraf dist && rollup -c rollup.config.mjs -w",
    "build": "rimraf dist && cross-env mode=prod rollup -c rollup.config.mjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserslist": [
    "last 2 versions",
    "> 0.01% in cn",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "browserslist": "^4.19.1",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.63.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
